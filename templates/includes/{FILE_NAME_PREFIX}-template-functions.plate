if ( ! function_exists( '{PLUGIN_HOOK_CLASS_NAME}_quantity_input' ) ) {

	/**
	 * Output the quantity input for add to cart forms.
	 *
	 * @param  array $args Args for the input
	 * @param  WC_Product|null $product
	 * @param  boolean $echo Whether to return or echo|string
	 *
	 * @return string
	 */
	function {PLUGIN_HOOK_CLASS_NAME}_quantity_input( $args = array(), $product = null, $echo = true ) {
		if ( is_null( $product ) ) {
			$product = $GLOBALS['product'];
		}

		$defaults = array(
			'input_name'  => 'quantity',
			'input_value' => '1',
			'max_value'   => apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_quantity_input_max', -1, $product ),
			'min_value'   => apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_quantity_input_min', 0, $product ),
			'step'        => apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_quantity_input_step', 1, $product ),
			'pattern'     => apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_quantity_input_pattern', has_filter( '{PLUGIN_HOOK_CLASS_NAME}_stock_amount', 'intval' ) ? '[0-9]*' : '' ),
			'inputmode'   => apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_quantity_input_inputmode', has_filter( '{PLUGIN_HOOK_CLASS_NAME}_stock_amount', 'intval' ) ? 'numeric' : '' ),
		);

		$args = apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_quantity_input_args', wp_parse_args( $args, $defaults ), $product );

		// Apply sanity to min/max args - min cannot be lower than 0.
		$args['min_value'] = max( $args['min_value'], 0 );
		$args['max_value'] = 0 < $args['max_value'] ? $args['max_value'] : '';

		// Max cannot be lower than min if defined.
		if ( '' !== $args['max_value'] && $args['max_value'] < $args['min_value'] ) {
			$args['max_value'] = $args['min_value'];
		}

		ob_start();

		{FILE_NAME_PREFIX}_get_template( 'global/quantity-input.php', $args );

		if ( $echo ) {
			echo ob_get_clean();
		} else {
			return ob_get_clean();
		}
	}
}

