if ( ! function_exists( '{PLUGIN_HOOK_CLASS_NAME}_product_archive_description' ) ) {

	/**
	 * Show a shop page description on product archives.
	 *
	 * @subpackage	Archives
	 */
	function {PLUGIN_HOOK_CLASS_NAME}_product_archive_description() {
		// Don't display the description on search results page
		if ( is_search() ) {
			return;
		}

		if ( is_post_type_archive( 'product' ) && 0 === absint( get_query_var( 'paged' ) ) ) {
			$shop_page   = get_post( {FILE_NAME_PREFIX}_get_page_id( 'shop' ) );
			if ( $shop_page ) {
				$description = {FILE_NAME_PREFIX}_format_content( $shop_page->post_content );
				if ( $description ) {
					echo '<div class="page-description">' . $description . '</div>';
				}
			}
		}
	}
}

