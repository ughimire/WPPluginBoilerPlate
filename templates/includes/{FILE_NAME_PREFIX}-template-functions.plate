/**
 * Get HTML to show product stock.
 * @since  3.0.0
 * @param  WC_Product $product
 * @return string
 */
function {FILE_NAME_PREFIX}_get_stock_html( $product ) {

	$html = '';
	$availability = $product->get_availability();

	if ( ! empty( $availability['availability'] ) ) {
		ob_start();

		{FILE_NAME_PREFIX}_get_template( 'single-product/stock.php', array(
			'product'      => $product,
			'class'        => $availability['class'],
			'availability' => $availability['availability'],
		) );

		$html = ob_get_clean();
	}

	if ( has_filter( '{PLUGIN_HOOK_CLASS_NAME}_stock_html' ) ) {
		{FILE_NAME_PREFIX}_deprecated_function( 'The {PLUGIN_HOOK_CLASS_NAME}_stock_html filter', '', '{PLUGIN_HOOK_CLASS_NAME}_get_stock_html' );
		$html = apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_stock_html', $html, $availability['availability'], $product );
	}

	return apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_get_stock_html', $html, $product );
}

