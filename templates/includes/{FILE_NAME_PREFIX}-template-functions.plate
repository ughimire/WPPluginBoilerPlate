if ( ! function_exists( '{PLUGIN_HOOK_CLASS_NAME}_upsell_display' ) ) {

	/**
	 * Output product up sells.
	 *
	 * @param int $limit (default: -1)
	 * @param int $columns (default: 4)
	 * @param string $orderby Supported values - rand, title, ID, date, modified, menu_order, price.
	 * @param string $order Sort direction.
	 */
	function {PLUGIN_HOOK_CLASS_NAME}_upsell_display( $limit = '-1', $columns = 4, $orderby = 'rand', $order = 'desc' ) {
		global $product, ${PLUGIN_HOOK_CLASS_NAME}_loop;

		if ( ! $product ) {
			return;
		}

		// Handle the legacy filter which controlled posts per page etc.
		$args = apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_upsell_display_args', array(
			'posts_per_page' => $limit,
			'orderby'        => $orderby,
			'columns'        => $columns,
		) );
		${PLUGIN_HOOK_CLASS_NAME}_loop['name']    = 'up-sells';
		${PLUGIN_HOOK_CLASS_NAME}_loop['columns'] = apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_upsells_columns', isset( $args['columns'] ) ? $args['columns'] : $columns );
		$orderby                     = apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_upsells_orderby', isset( $args['orderby'] ) ? $args['orderby'] : $orderby );
		$limit                       = apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_upsells_total', isset( $args['posts_per_page'] ) ? $args['posts_per_page'] : $limit );

		// Get visble upsells then sort them at random, then limit result set.
		$upsells = {FILE_NAME_PREFIX}_products_array_orderby( array_filter( array_map( '{FILE_NAME_PREFIX}_get_product', $product->get_upsell_ids() ), '{FILE_NAME_PREFIX}_products_array_filter_visible' ), $orderby, $order );
		$upsells = $limit > 0 ? array_slice( $upsells, 0, $limit ) : $upsells;

		{FILE_NAME_PREFIX}_get_template( 'single-product/up-sells.php', array(
			'upsells' => $upsells,

			// Not used now, but used in previous version of up-sells.php.
			'posts_per_page' => $limit,
			'orderby'        => $orderby,
			'columns'        => $columns,
		) );
	}
}

