		$args['related_products'] = array_filter( array_map( '{FILE_NAME_PREFIX}_get_product', {FILE_NAME_PREFIX}_get_related_products( $product->get_id(), $args['posts_per_page'], $product->get_upsell_ids() ) ), '{FILE_NAME_PREFIX}_products_array_filter_visible' );

		// Handle orderby.
		$args['related_products'] = {FILE_NAME_PREFIX}_products_array_orderby( $args['related_products'], $args['orderby'], $args['order'] );

		// Set global loop values.
		${PLUGIN_HOOK_CLASS_NAME}_loop['name']    = 'related';
		${PLUGIN_HOOK_CLASS_NAME}_loop['columns'] = apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_related_products_columns', $args['columns'] );

		{FILE_NAME_PREFIX}_get_template( 'single-product/related.php', $args );
