/**
 * When the_post is called, put product data into a global.
 *
 * @param mixed $post
 * @return WC_Product
 */
function {FILE_NAME_PREFIX}_setup_product_data( $post ) {
	unset( $GLOBALS['product'] );

	if ( is_int( $post ) ) {
		$post = get_post( $post );
	}

	if ( empty( $post->post_type ) || ! in_array( $post->post_type, array( 'product', 'product_variation' ) ) ) {
		return;
	}

	$GLOBALS['product'] = {FILE_NAME_PREFIX}_get_product( $post );

	return $GLOBALS['product'];
}
add_action( 'the_post', '{FILE_NAME_PREFIX}_setup_product_data' );

