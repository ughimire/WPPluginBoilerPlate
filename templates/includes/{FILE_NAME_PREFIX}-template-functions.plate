/** Loop ******************************************************************/

if ( ! function_exists( '{PLUGIN_HOOK_CLASS_NAME}_page_title' ) ) {

	/**
	 * {PLUGIN_HOOK_CLASS_NAME}_page_title function.
	 *
	 * @param  bool $echo
	 * @return string
	 */
	function {PLUGIN_HOOK_CLASS_NAME}_page_title( $echo = true ) {

		if ( is_search() ) {
			$page_title = sprintf( __( 'Search results: &ldquo;%s&rdquo;', '{TEXT_DOMAIN}' ), get_search_query() );

			if ( get_query_var( 'paged' ) ) {
				$page_title .= sprintf( __( '&nbsp;&ndash; Page %s', '{TEXT_DOMAIN}' ), get_query_var( 'paged' ) );
			}
		} elseif ( is_tax() ) {

			$page_title = single_term_title( "", false );

		} else {

			$shop_page_id = {FILE_NAME_PREFIX}_get_page_id( 'shop' );
			$page_title   = get_the_title( $shop_page_id );

		}

		$page_title = apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_page_title', $page_title );

		if ( $echo ) {
			echo $page_title;
		} else {
			return $page_title;
		}
	}
}

if ( ! function_exists( '{PLUGIN_HOOK_CLASS_NAME}_product_loop_start' ) ) {

	/**
	 * Output the start of a product loop. By default this is a UL.
	 *
	 * @param bool $echo
	 * @return string
	 */
	function {PLUGIN_HOOK_CLASS_NAME}_product_loop_start( $echo = true ) {
		ob_start();
		$GLOBALS['{PLUGIN_HOOK_CLASS_NAME}_loop']['loop'] = 0;
		{FILE_NAME_PREFIX}_get_template( 'loop/loop-start.php' );
		if ( $echo ) {
			echo ob_get_clean();
		} else {
			return ob_get_clean();
		}
	}
}
