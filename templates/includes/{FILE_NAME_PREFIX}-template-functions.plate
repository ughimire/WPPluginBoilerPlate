/**
 * Products RSS Feed.
 * @deprecated 2.6
 * @access public
 */
function {FILE_NAME_PREFIX}_products_rss_feed() {
	// Product RSS
	if ( is_post_type_archive( 'product' ) || is_singular( 'product' ) ) {

		$feed = get_post_type_archive_feed_link( 'product' );

		echo '<link rel="alternate" type="application/rss+xml"  title="' . esc_attr__( 'New products', '{TEXT_DOMAIN}' ) . '" href="' . esc_url( $feed ) . '" />';

	} elseif ( is_tax( 'product_cat' ) ) {

		$term = get_term_by( 'slug', esc_attr( get_query_var( 'product_cat' ) ), 'product_cat' );

		if ( $term ) {
			$feed = add_query_arg( 'product_cat', $term->slug, get_post_type_archive_feed_link( 'product' ) );
			echo '<link rel="alternate" type="application/rss+xml"  title="' . sprintf( esc_attr__( 'New products added to %s', '{TEXT_DOMAIN}' ), $term->name ) . '" href="' . esc_url( $feed ) . '" />';
		}
	} elseif ( is_tax( 'product_tag' ) ) {

		$term = get_term_by( 'slug', esc_attr( get_query_var( 'product_tag' ) ), 'product_tag' );

		if ( $term ) {
			$feed = add_query_arg( 'product_tag', $term->slug, get_post_type_archive_feed_link( 'product' ) );
			echo '<link rel="alternate" type="application/rss+xml"  title="' . sprintf( esc_attr__( 'New products tagged %s', '{TEXT_DOMAIN}' ), urlencode( $term->name ) ) . '" href="' . esc_url( $feed ) . '" />';
		}
	}
}

/**
 * Output generator tag to aid debugging.
 *
 * @access public
 *
 * @param string $gen
 * @param string $type
 *
 * @return string
 */
function {FILE_NAME_PREFIX}_generator_tag( $gen, $type ) {
	switch ( $type ) {
		case 'html':
			$gen .= "\n" . '<meta name="generator" content="WooCommerce ' . esc_attr( WC_VERSION ) . '">';
			break;
		case 'xhtml':
			$gen .= "\n" . '<meta name="generator" content="WooCommerce ' . esc_attr( WC_VERSION ) . '" />';
			break;
	}
	return $gen;
}

/**
 * Add body classes for WC pages.
 *
 * @param  array $classes
 * @return array
 */
function {FILE_NAME_PREFIX}_body_class( $classes ) {
	$classes = (array) $classes;

	if ( is_woocommerce() ) {

		$classes[] = '{TEXT_DOMAIN}';
		$classes[] = 'woocommerce-page';

	} elseif ( is_checkout() ) {

		$classes[] = 'woocommerce-checkout';
		$classes[] = 'woocommerce-page';

	} elseif ( is_cart() ) {

		$classes[] = 'woocommerce-cart';
		$classes[] = 'woocommerce-page';

	} elseif ( is_account_page() ) {

		$classes[] = 'woocommerce-account';
		$classes[] = 'woocommerce-page';

	}

	if ( is_store_notice_showing() ) {
		$classes[] = 'woocommerce-demo-store';
	}

	foreach ( {PLUGIN_CLASS_PREFIX}()->query->query_vars as $key => $value ) {
		if ( is_{FILE_NAME_PREFIX}_endpoint_url( $key ) ) {
			$classes[] = 'woocommerce-' . sanitize_html_class( $key );
		}
	}

	return array_unique( $classes );
}

/**
 * Display the classes for the product cat div.
 *
 * @since 2.4.0
 * @param string|array $class One or more classes to add to the class list.
 * @param object $category object Optional.
 */
function {FILE_NAME_PREFIX}_product_cat_class( $class = '', $category = null ) {
	// Separates classes with a single space, collates classes for post DIV
	echo 'class="' . esc_attr( join( ' ', {FILE_NAME_PREFIX}_get_product_cat_class( $class, $category ) ) ) . '"';
}

/**
 * Get classname for loops based on ${PLUGIN_HOOK_CLASS_NAME}_loop global.
 * @since 2.6.0
 * @return string
 */
function {FILE_NAME_PREFIX}_get_loop_class() {
	global ${PLUGIN_HOOK_CLASS_NAME}_loop;

	${PLUGIN_HOOK_CLASS_NAME}_loop['loop']    = ! empty( ${PLUGIN_HOOK_CLASS_NAME}_loop['loop'] ) ? ${PLUGIN_HOOK_CLASS_NAME}_loop['loop'] + 1   : 1;
	${PLUGIN_HOOK_CLASS_NAME}_loop['columns'] = max( 1, ! empty( ${PLUGIN_HOOK_CLASS_NAME}_loop['columns'] ) ? ${PLUGIN_HOOK_CLASS_NAME}_loop['columns'] : apply_filters( 'loop_shop_columns', 4 ) );

	if ( 0 === ( ${PLUGIN_HOOK_CLASS_NAME}_loop['loop'] - 1 ) % ${PLUGIN_HOOK_CLASS_NAME}_loop['columns'] || 1 === ${PLUGIN_HOOK_CLASS_NAME}_loop['columns'] ) {
		return 'first';
	} elseif ( 0 === ${PLUGIN_HOOK_CLASS_NAME}_loop['loop'] % ${PLUGIN_HOOK_CLASS_NAME}_loop['columns'] ) {
		return 'last';
	} else {
		return '';
	}
}

/**
 * Get the classes for the product cat div.
 *
 * @since 2.4.0
 *
 * @param string|array $class One or more classes to add to the class list.
 * @param object $category object Optional.
 *
 * @return array
 */
function {FILE_NAME_PREFIX}_get_product_cat_class( $class = '', $category = null ) {
	$classes   = is_array( $class ) ? $class : array_map( 'trim', explode( ' ', $class ) );
	$classes[] = 'product-category';
	$classes[] = 'product';
	$classes[] = {FILE_NAME_PREFIX}_get_loop_class();
	$classes   = apply_filters( 'product_cat_class', $classes, $class, $category );

	return array_unique( array_filter( $classes ) );
}

/**
 * Adds extra post classes for products.
 *
 * @since 2.1.0
 * @param array $classes
 * @param string|array $class
 * @param int $post_id
 * @return array
 */
function {FILE_NAME_PREFIX}_product_post_class( $classes, $class = '', $post_id = '' ) {
	if ( ! $post_id || ! in_array( get_post_type( $post_id ), array( 'product', 'product_variation' ) ) ) {
		return $classes;
	}

	$product = {FILE_NAME_PREFIX}_get_product( $post_id );

	if ( $product ) {
		$classes[] = 'product';
		$classes[] = {FILE_NAME_PREFIX}_get_loop_class();
		$classes[] = $product->get_stock_status();

		if ( $product->is_on_sale() ) {
			$classes[] = 'sale';
		}
		if ( $product->is_featured() ) {
			$classes[] = 'featured';
		}
		if ( $product->is_downloadable() ) {
			$classes[] = 'downloadable';
		}
		if ( $product->is_virtual() ) {
			$classes[] = 'virtual';
		}
		if ( $product->is_sold_individually() ) {
			$classes[] = 'sold-individually';
		}
		if ( $product->is_taxable() ) {
			$classes[] = 'taxable';
		}
		if ( $product->is_shipping_taxable() ) {
			$classes[] = 'shipping-taxable';
		}
		if ( $product->is_purchasable() ) {
			$classes[] = 'purchasable';
		}
		if ( $product->get_type() ) {
			$classes[] = "product-type-" . $product->get_type();
		}
		if ( $product->is_type( 'variable' ) ) {
			if ( ! $product->get_default_attributes() ) {
				$classes[] = 'has-default-attributes';
			}
			if ( $product->has_child() ) {
				$classes[] = 'has-children';
			}
		}
	}

	if ( false !== ( $key = array_search( 'hentry', $classes ) ) ) {
		unset( $classes[ $key ] );
	}

	return $classes;
}

/**
 * Outputs hidden form inputs for each query string variable.
 * @since 3.0.0
 *
 * @param array $values Name value pairs.
 * @param array $exclude Keys to exclude.
 * @param string $current_key Current key we are outputting.
 * @param bool $return
 * @return string
 */
function {FILE_NAME_PREFIX}_query_string_form_fields( $values = null, $exclude = array(), $current_key = '', $return = false ) {
	if ( is_null( $values ) ) {
		$values = $_GET;
	}
	$html = '';

	foreach ( $values as $key => $value ) {
		if ( in_array( $key, $exclude, true ) ) {
			continue;
		}
		if ( $current_key ) {
			$key = $current_key . '[' . $key . ']';
		}
		if ( is_array( $value ) ) {
			$html .= {FILE_NAME_PREFIX}_query_string_form_fields( $value, $exclude, $key, true );
		} else {
			$html .= '<input type="hidden" name="' . esc_attr( $key ) . '" value="' . esc_attr( $value ) . '" />';
		}
	}

	if ( $return ) {
		return $html;
	} else {
		echo $html;
	}
}

/** Template pages ********************************************************/

if ( ! function_exists( '{PLUGIN_HOOK_CLASS_NAME}_content' ) ) {

	/**
	 * Output WooCommerce content.
	 *
	 * This function is only used in the optional 'woocommerce.php' template.
	 * which people can add to their themes to add basic woocommerce support.
	 * without hooks or modifying core templates.
	 *
	 */
	function {PLUGIN_HOOK_CLASS_NAME}_content() {

		if ( is_singular( 'product' ) ) {

			while ( have_posts() ) : the_post();

				{FILE_NAME_PREFIX}_get_template_part( 'content', 'single-product' );

			endwhile;

		} else { ?>

			<?php if ( apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_show_page_title', true ) ) : ?>

				<h1 class="page-title"><?php {PLUGIN_HOOK_CLASS_NAME}_page_title(); ?></h1>

			<?php endif; ?>

			<?php do_action( '{PLUGIN_HOOK_CLASS_NAME}_archive_description' ); ?>

			<?php if ( have_posts() ) : ?>

				<?php do_action( '{PLUGIN_HOOK_CLASS_NAME}_before_shop_loop' ); ?>

				<?php {PLUGIN_HOOK_CLASS_NAME}_product_loop_start(); ?>

					<?php {PLUGIN_HOOK_CLASS_NAME}_product_subcategories(); ?>

					<?php while ( have_posts() ) : the_post(); ?>

						<?php {FILE_NAME_PREFIX}_get_template_part( 'content', 'product' ); ?>

					<?php endwhile; // end of the loop. ?>

				<?php {PLUGIN_HOOK_CLASS_NAME}_product_loop_end(); ?>

				<?php do_action( '{PLUGIN_HOOK_CLASS_NAME}_after_shop_loop' ); ?>

			<?php elseif ( ! {PLUGIN_HOOK_CLASS_NAME}_product_subcategories( array( 'before' => {PLUGIN_HOOK_CLASS_NAME}_product_loop_start( false ), 'after' => {PLUGIN_HOOK_CLASS_NAME}_product_loop_end( false ) ) ) ) : ?>

				<?php do_action( '{PLUGIN_HOOK_CLASS_NAME}_no_products_found' ); ?>

			<?php endif;

		}
	}
}

