if ( ! function_exists( '{PLUGIN_HOOK_CLASS_NAME}_catalog_ordering' ) ) {

	/**
	 * Output the product sorting options.
	 *
	 * @subpackage	Loop
	 */
	function {PLUGIN_HOOK_CLASS_NAME}_catalog_ordering() {
		global $wp_query;

		if ( 1 === (int) $wp_query->found_posts || ! {PLUGIN_HOOK_CLASS_NAME}_products_will_display() || $wp_query->is_search() ) {
			return;
		}

		$orderby                 = isset( $_GET['orderby'] ) ? {FILE_NAME_PREFIX}_clean( $_GET['orderby'] ) : apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_default_catalog_orderby', get_option( '{PLUGIN_HOOK_CLASS_NAME}_default_catalog_orderby' ) );
		$show_default_orderby    = 'menu_order' === apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_default_catalog_orderby', get_option( '{PLUGIN_HOOK_CLASS_NAME}_default_catalog_orderby' ) );
		$catalog_orderby_options = apply_filters( '{PLUGIN_HOOK_CLASS_NAME}_catalog_orderby', array(
			'menu_order' => __( 'Default sorting', '{TEXT_DOMAIN}' ),
			'popularity' => __( 'Sort by popularity', '{TEXT_DOMAIN}' ),
			'rating'     => __( 'Sort by average rating', '{TEXT_DOMAIN}' ),
			'date'       => __( 'Sort by newness', '{TEXT_DOMAIN}' ),
			'price'      => __( 'Sort by price: low to high', '{TEXT_DOMAIN}' ),
			'price-desc' => __( 'Sort by price: high to low', '{TEXT_DOMAIN}' ),
		) );

		if ( ! $show_default_orderby ) {
			unset( $catalog_orderby_options['menu_order'] );
		}

		if ( 'no' === get_option( '{PLUGIN_HOOK_CLASS_NAME}_enable_review_rating' ) ) {
			unset( $catalog_orderby_options['rating'] );
		}

		{FILE_NAME_PREFIX}_get_template( 'loop/orderby.php', array( 'catalog_orderby_options' => $catalog_orderby_options, 'orderby' => $orderby, 'show_default_orderby' => $show_default_orderby ) );
	}
}

